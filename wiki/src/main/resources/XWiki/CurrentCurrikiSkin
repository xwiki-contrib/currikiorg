&lt;script language="javascript"&gt;
var lasttab=document.getElementById('tabcourses');
function setSearchContext(contexte,tab){
 if(lasttab!=null){
  lasttab.className="tab-d";
 }
 tab.className="tab-a";
 lasttab=tab;
 document.searchform.context=contexte;
}
&lt;/script&gt;
