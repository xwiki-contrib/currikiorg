&lt;script type="text/javascript"&gt;
var tabSelected = "courses";
function changeImage(divObj) {
  #set($courseblueurl = $xwiki.getSkinFile("course_blue.gif"))
  #set($orangeupurl = $xwiki.getSkinFile("orange_up.gif"))
  #set($greenupurl = $xwiki.getSkinFile("green_up.gif"))
  #set($resoblueurl = $xwiki.getSkinFile("reso_blue.gif"))
  #set($blueupurl = $xwiki.getSkinFile("blue_up.gif"))
  #set($groupblueurl = $xwiki.getSkinFile("group_blue.gif"))
  #set($leftimgurl = $xwiki.getSkinFile("left_img.gif"))
  #set($rightimgurl = $xwiki.getSkinFile("right_img.gif"))
  #set($gourl = $xwiki.getSkinFile("go.gif"))
	if(divObj.id == "tab1") {
	document.getElementById(divObj.id).innerHTML = "&lt;img src=\"$courseblueurl\"&gt;";
	document.getElementById("tab2").innerHTML = "&lt;img src=\"$orangeupurl\"&gt;";
	document.getElementById("tab3").innerHTML = "&lt;img src=\"$greenupurl\"&gt;";
	tabSelected = "courses";
	}
	else if (divObj.id == "tab2") {
	document.getElementById(divObj.id).innerHTML = "&lt;img src=\"$resoblueurl\"&gt;";
	document.getElementById("tab1").innerHTML = "&lt;img src=\"$blueupurl\"&gt;";
	document.getElementById("tab3").innerHTML = "&lt;img src=\"$greenupurl\"&gt;";
	tabSelected = "resources";
	}
	else if (divObj.id == "tab3") {
	document.getElementById(divObj.id).innerHTML = "&lt;img src=\"$groupblueurl\"&gt;";
	document.getElementById("tab2").innerHTML = "&lt;img src=\"$orangeupurl\"&gt;";
	document.getElementById("tab1").innerHTML = "&lt;img src=\"$blueupurl\"&gt;";
	tabSelected = "group";
	}
        }
        
function tabSearch(){
        var queryText = document.getElementById("tabSearchField");
        if( queryText != undefined ){
            queryText = queryText.value;
        }else{
             queryText = "";
        }
	window.location="http://www.google.com/search?q="+tabSelected+"+"+queryText;
}
&lt;/script&gt;
#if(($context.action=="inline")||($context.action=="edit") || ($context.action=="admin"))
&lt;script type="text/javascript"&gt;
 var cancelEditNextOnunload;
 var cancelCancelEditFlag = true;
 var  lockEditFlag = false;

 function cancelEditResponse(){
 }

 function cancelEdit(){
     if (cancelCancelEditFlag) {
       var surl = "$doc.getExternalURL("cancel", "ajax=1")";
       var myAjax = new Ajax.Request(
                 surl,
                 {
                     method: 'get',
                     parameters: "",
                     asynchronous: true
                 });
        cancelCancelEditFlag = false;
     }
     if (cancelEditNextOnunload!=null)
           cancelEditNextOnunload();
 }

 function lockEdit(){
    if (lockEditFlag==false) {
#if($context.action=="inline")
      var surl = "$doc.getExternalURL("lock")?action=inline&amp;ajax=1&amp;${docvariant.replace("&amp;amp;", "&amp;")}";
#else
      var surl = "$doc.getExternalURL("lock")?ajax=1&amp;${docvariant.replace("&amp;amp;", "&amp;")}";
#end
      var myAjax = new Ajax.Request(
                surl,
                {
                    method: 'get',
                    parameters: "",
                    asynchronous: true
                });
       lockEditFlag = false;
    }
 }

 function prepareCancelEdit() {
     cancelEditNextOnunload = document.onunload;
     document.onunload = cancelEdit;
     lockEdit();
 }
 function cancelCancelEdit() {
     cancelCancelEditFlag = false;
 }
}
&lt;/script&gt;
